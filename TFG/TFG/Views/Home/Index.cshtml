<div class="sidebar">
    <div class="inputs text-left">
        <input id="start" type="text" name="start" placeholder="Origen..." autocomplete="off" />
        <input id="end" class="mt-3" type="text" name="end" placeholder="Destino..." />
        <div class="mt-2 text-center">
            <button id="submit" class="btn btn-light" type="submit">
                <i class="fa-solid fa-route fa-lg"></i>
            </button>
        </div>
    </div>
    <hr class="new-section" />
    <div id="results" class="hide">
        <button id="public-transport" class="custom-accordion"><i class="fa-solid fa-train-subway fa-lg"></i><i class="fa-solid fa-bus-simple fa-lg ml-1"></i></button>
        <div class="panel">
            <div id="info-pt">
                <p>Nada para mostrar</p>
            </div>
            <div class="text-center mb-2">
                <button id="publictransport-route" class="btn-route btn btn-light"><b>Generar ruta</b></button>
            </div>
        </div>
        <button id="car" class="custom-accordion"><i class="fa-solid fa-car fa-lg"></i></button>
        <div class="panel">
            <div id="info-car">
                <p>Nada para mostrar</p>
            </div>

            <div class="text-center mb-2">
                <button id="car-route" class="btn-route btn btn-light"><b>Generar ruta</b></button>
            </div>
        </div>
        <button id="bicycle" class="custom-accordion"><i class="fa-solid fa-bicycle fa-lg"></i></button>
        <div class="panel">
            <div id="info-bicycle">
                <p>Nada para mostrar</p>
            </div>
            <div class="text-center mb-2">
                <button id="bicycle-route" class="btn-route btn btn-light"><b>Generar ruta</b></button>
            </div>
        </div>
        <button id="pedestrian" class="custom-accordion"><i class="fa-solid fa-person-walking fa-lg"></i></button>
        <div class="panel">
            <div id="info-pedestrian">
                <p>Nada para mostrar</p>
            </div>
            <div class="text-center mb-2">
                <button id="foot-route" class="btn-route btn btn-light"><b>Generar ruta</b></button>
            </div>
        </div>


    </div>
</div>


<div id="map">
</div>


